<div class="wrapper">
		<div class="row">
			<div class="newsletter-head-dashboard">
            <h1 data-aos="fade-right"data-aos-duration="1500">News Letter</h1>
        </div>
        <div class="newsletter-main" data-aos="zoom-in-up" data-aos-duration="600">
            <div class="newsletter-head"></div>
            <form action="">
                <div class="newsletter-form-1-1">
                    <div class="newsletter-form-1">
                        <label>Email (User)</label>
                        <select name="" id="">
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                        </select>
                    </div>
                    <div class="newsletter-form-2">
                        <label>Email (Subcribers)</label>
                        <select name="" id="">
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                        </select>
                    </div>
                </div>
                <div class="newsletter-form-3">
                    <label>Newsletter Subject :</label>
                    <input type="text">
                </div>
                <div class="newsletter-form-4">
                    <label>Newsletter Content :</label>
                    <textarea name="" id="" cols="30" rows="10"></textarea>
                </div>

                <div class="newsletter-btn">
                    <input type="button" value="SEND">
                </div>
            </form>
        </div>
</div>
</div>
